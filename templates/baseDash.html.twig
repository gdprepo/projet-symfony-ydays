<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
		<meta http-equiv="X-UA-Compatible" content="ie=edge"/>
		<title>
			{% block title %}{% endblock %}
		</title>

		{% block stylesheets %}
			{{ encore_entry_link_tags('css') }}
			{{ encore_entry_link_tags('app') }}
			<link href="{{ asset('css/vip.css') }}" rel="stylesheet">
			<link href="{{ asset('css/styleMenu.css') }}" rel="stylesheet">
			<link href="{{ asset('css/style.css') }}" rel="stylesheet">


		{% endblock %}


		<style>
			@media(min-width: 768px) {
				.navbar-brand.abs {
					position: absolute;
					width: 100%;
					left: 0;
					top: 5px;
					text-align: center;
				}


			}
			.nav-link {
				color: black !important
			}

			.center-con {
				margin-top: -28px;
				display: flex;
				align-items: center;
				justify-content: center;
				transform: rotate(90deg);
			}


			.round {
				position: absolute;
				border: 2px solid white;
				width: 40px;
				height: 40px;
				border-radius: 100%;

			}

			#cta {
				width: 100%;
				cursor: pointer;
				position: absolute;
			}

			#cta .arrow {
				left: 30%;
				top: 12px
			}
			.arrow {
				position: absolute;
				bottom: 0;
				margin-left: 0;
				width: 12px;
				height: 12px;
				background-size: contain;
				top: 15px;
			}
			.segunda {
				margin-left: 8px;
			}
			.next {
				background-image: url('data:image/svg+xml;base64,');
			}

			@keyframes bounceAlpha {
				0% {
					opacity: 1;
					transform: translateX(0px) scale(1);
				}
				25% {
					opacity: 0;
					transform: translateX(10px) scale(0.9);
				}
				26% {
					opacity: 0;
					transform: translateX(-10px) scale(0.9);
				}
				55% {
					opacity: 1;
					transform: translateX(0px) scale(1);
				}
			}

			.bounceAlpha {
				animation-name: bounceAlpha;
				animation-duration: 1.4s;
				animation-iteration-count: infinite;
				animation-timing-function: linear;
			}

			.arrow.primera.bounceAlpha {
				animation-name: bounceAlpha;
				animation-duration: 1.4s;
				animation-delay: 0.2s;
				animation-iteration-count: infinite;
				animation-timing-function: linear;
			}

			.round:hover .arrow {
				animation-name: bounceAlpha;
				animation-duration: 1.4s;
				animation-iteration-count: infinite;
				animation-timing-function: linear;
			}
			.round:hover .arrow.primera {
				animation-name: bounceAlpha;
				animation-duration: 1.4s;
				animation-delay: 0.2s;
				animation-iteration-count: infinite;
				animation-timing-function: linear;
			}

			body {
				background-color: black;
				color: white
			}

			hr {
				background-color: white
			}

			.footerlinks {
				list-style: none;
				margin: 10px 5px 5px;
				padding: 0;

				color: grey;
				letter-spacing: 3px
			}

			.footercontact {
				list-style: none;
				margin: 10px 0;
				color: grey;
				letter-spacing: 1px
			}

			.footercontact li {
				margin: 10px 0;
			}

			footer a {
				color: grey;
			}

			footer a:link {
				color: grey;
				text-decoration: none;
			}

			footer a:hover {
				color: white;
			}
		</style>
		<link
		href="https://fonts.googleapis.com/css?family=Lato:100,300,400,700,900" rel="stylesheet"/>
		<!-- Axentix CSS minified version -->
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.0.0/dist/css/axentix.min.css">
		<script src="https://kit.fontawesome.com/bdd89edb33.js"></script>
	</head>
	<body>

		<aside class="sidebar">

			<div class="side-inner">

				<div class="logo">
					<span>L</span>

				</div>

				<div class="nav-menu">
					<div class="shopSide">
						<h3 style="text-align:center; letter-spacing: 5px; font-size: 20px; margin-bottom: 15px"></h3>
						<ul>
							<li>
								<a href="/vip">Abonnements VIP</a>
							</li>
							<li>
								<a href="/dashboard/pronos">List des Pronos</a>
							</li>
							<li>
								<a href="{{ path('dashboard_users') }}">List des Users</a>
							</li>
							{# <li>
																													<a href="#">Categorie3</a>
																												</li> #}

						</ul>
						<hr>

					</div>


					<ul>
						<li>
							<a href="/">Home</a>
						</li>
						{% if app.user %}
							<li>
								<a href="/dashboard">Dashboard</a>
							</li>
							<li>
								<a href="/logout">Se deconnecter</a>
							</li>
						{% else %}
							<li>
								<a href="/login">Se connecter</a>
							</li>
							<li>
								<a href="/register">S'inscrire</a>
							</li>
						{% endif %}

					</ul>
				</div>

			</div>

		</aside>
		<main>
			<div style="z-index: 10000">
				{% include '_includes/headerDash.html.twig' %}
			</div>

			<div style="z-index: 1"> {% block body %}{% endblock %}
				</div>


				{# {% include '_includes/footer.html.twig' %} #}
			</main>
			{% block javascripts %}
				{{ encore_entry_script_tags('app') }}
			{% endblock %}
		</body>
		<!-- Axentix JS minified version -->
		<script src="https://cdn.jsdelivr.net/npm/axentix@1.0.0/dist/js/axentix.min.js"></script>
		<!-- Grix system -->
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.0.0/dist/css/grix.min.css">
		<script src="{{ asset('js/jquery-3.3.1.min.js') }}"></script>

		<!-- Vendor JS Files -->
		<script src="{{ asset('jquery/jquery.min.js') }}"></script>
		{# <script src="{{ asset('bootstrap/js/bootstrap.bundle.min.js') }} "></script> #}
		<script src="{{ asset('jquery.easing/jquery.easing.min.js') }}"></script>
		<script src="{{ asset('php-email-form/validate.js') }}"></script>
		<script src="{{ asset('aos/aos.js') }}"></script>
		<script src="{{ asset('isotope-layout/isotope.pkgd.min.js') }}"></script>
		<script src="{{ asset('owl.carousel/owl.carousel.min.js') }}"></script>

		<!-- Template Main JS File -->
		<script src="{{ asset('js/mainShop.js') }}"></script>


		<script src="{{ asset('js/popper.min.js') }}"></script>
		<script src="{{ asset('js/bootstrap.min.js') }}"></script>
		<script src="{{ asset('js/main.js') }}"></script>

		<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>

		<script>
			$('.round').click(function (e) {
e.preventDefault();
e.stopPropagation();
$('.arrow').toggleClass('bounceAlpha');
$(window).scrollTop($('#prono').offset().top);

});
		</script>

	</html>
